    "vue": "^3.0.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "c8": "^7.11.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "only-allow": "^1.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.43.4",
